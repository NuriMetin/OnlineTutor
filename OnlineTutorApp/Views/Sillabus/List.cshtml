@model CourseVM

@{
    ViewData["Title"] = "List";
}

<link rel="stylesheet" href="~/css/course.css">

<div class="main-container pb-5">
    <div id="course" class="col-md-12">
        <div class="container-fluid">
            <div class="row">
                <div class="section-title col-md-12 col-sm-12 pb-5 pt-5" style="text-align:center;">
                    <h2>Sillabus</h2>
                </div>
                @foreach (var course in Model.SillabusList)
                {
                    <div class="course col-md-4 col-sm-12">
                        <div class="course-thumb">
                            <h5>@course.Title</h5>
                            <span>@course.PublishDate</span>
                            <a href="~/sillabus/@course.Path" class="btn btn-light">Download</a>
                        </div>
                    </div>
                }
            </div>
        </div>
    </div>
</div>



